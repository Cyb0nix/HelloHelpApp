<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowAnimationStyle">@null</item>
    </style>

    <style name="MenuStyle" parent="Theme.AppCompat.Light">
        <item name="android:background">@android:color/white</item>
    </style>

    <style name="MenuStyle2" parent="Theme.AppCompat.Light">
        <item name="android:background">@color/colorPrimaryDark</item>
    </style>

    <style name="AppTheme.Launcher">
    <item name="android:windowBackground">@xml/launch_screen</item>
    <!-- Optional, on Android 5+ you can modify the colorPrimaryDark color to match the windowBackground color for further branding-->
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

</resources>
